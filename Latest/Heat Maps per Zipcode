#Secondary Plot 
library(tidyverse)
library(leaflet)
#install.packages("leaflet.esri")
library(leaflet.esri)
library(leaflet.extras)



firstheat<- leaflet(first) %>% addProviderTiles(providers$CartoDB.DarkMatter) %>%
  addWebGLHeatmap(~ first$longitude, ~ first$latitude, size = 60)  

secondheat <- leaflet(second) %>% addProviderTiles(providers$CartoDB.DarkMatter) %>%
  addWebGLHeatmap(~ second$longitude, ~ second$latitude, size = 60)  

